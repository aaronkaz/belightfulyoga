<dl class="info-list">
	
	<% model_class::FORM_COLUMNS.each do |column| %>
		<dt><%= model_class.human_attribute_name(column[0].to_sym) %></dt>
		<dd>
			<% if column[1] == 'association_select' || column[1] == 'datepicker' || column[1] == 'time' || column[1] == 'price' %>
				<%= render "admin/shared/show_fields/#{column[1]}", instance: instance, column: column[0] %>
			<% else %>
				<%= instance.send(column[0])%>
			<% end %>
		</dd>
	<% end %>
	
</dl>