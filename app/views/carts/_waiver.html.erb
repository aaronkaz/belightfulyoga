<link href="/stylesheets/waiver.css" media="all" rel="stylesheet" type="text/css" />



<%= error_messages_for(@cart) %>

<div class='WordSection1' style='width:700px;'>
  
  
<%= form_for @cart, :url => update_checkout_cart_path(@cart), :html => { :method => :put} do |f| %>
<%= hidden_field_tag :stage, @stage %>

<%= f.fields_for :waiver do |waiver| %>


<p class=MsoTitle><span style='font-size:14.0pt;text-decoration:none'>Belightful
Yoga, LLC</span></p>

<p class=MsoTitle><span style='font-size:14.0pt;text-decoration:none'>Classes/Workshops/Series</span></p>

<p class=MsoTitle style='margin-bottom:24.0pt'><span style='font-size:14.0pt'>Agreement
of Release and Waiver of Liability</span></p>


<br />

<div class=MsoNormal style='margin-bottom:6.0pt;line-height:150%'><span style='font-size:10.0pt;line-height:150%;font-family:Arial'>
First Name: <%= waiver.text_field :first_name, :style => "width:190px;" %> &nbsp; 
Middle Initial: <%= waiver.text_field :middle_initial, :style => "width:20px;" %> &nbsp; 
Last Name: <%= waiver.text_field :last_name, :style => "width:190px;" %> &nbsp; 
</span></div>

<div class=MsoNormal style='margin-bottom:6.0pt;line-height:150%'><span style='font-size:10.0pt;line-height:150%;font-family:Arial'>
